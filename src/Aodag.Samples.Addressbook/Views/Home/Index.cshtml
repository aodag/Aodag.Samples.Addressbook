@{
    // ViewBag.Title = "Index Page";
}
<a asp-action="new" class="btn btn-primary">New</a>
<table class="table">
    <tr>
        <th>FullName</th>
        <th>Email</th>
    </tr>
@foreach (var person in Model)
{
    <tr>
        <td>
            <a asp-action="edit" asp-route-id="@person.Id">@person.FullName</a>
        </td>
        <td>@person.Email</td>
    </tr>
}
</table>